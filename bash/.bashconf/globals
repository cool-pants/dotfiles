MAVEN_HOME=$BUILDS_PATH/apache-maven-3.9.8/bin;
RUST_PATH=$HOME/.cargo/bin
PATH=$PATH:$GO_PATH:$RUST_PATH:$JAVA_HOME:$LUA_PATH:$LUA_5_1_PATH:$BUILDS_PATH:$MAVEN_HOME
